<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="styles.css">
  <link href='https://fonts.googleapis.com/css?family=Allerta Stencil' rel='stylesheet'>
  <script src="script.js" defer></script>
  <title>Piano</title>
</head>
<body>

  
  <div class="page-wrap">
   
  <div class="buttons">

    <label class="rocker">
      <input type="checkbox" checked>
      <span class="switch-left">On</span>
      <span class="switch-right">Off</span>
    </label>

    <button class="rec-button btn">Record</button>
    <button class="play-button btn">Play</button>


    <audio id ="idAudio">
      <source src = "notes/Songg1.mp4"> </audio>
    <audio id ="idAudio2">
      <source src = "notes/Songg2.mp4"> </audio>
    <audio id ="idAudio3">
      <source src = "notes/Songg3.mp4"> </audio>

    <button onclick = "play_Audio()" class="rec-button btn">Song 1</button>
    <button onclick = "play_Audio2()" class="rec-button btn">Song 2</button>
    <button onclick = "play_Audio3()" class="rec-button btn">Song 3</button>
    <button onclick = "pause_Audio()" class="rec-button btn">Pause Song</button>

  </div>

  

  <div class="piano">
    <div data-note="C" class="key white"><h1>A</h1></div>
    <div data-note="Db" class="key black"><h2>W</h2></div>
    <div data-note="D" class="key white"><h1>S</h1></div>
    <div data-note="Eb" class="key black"><h2>E</h2></div>
    <div data-note="E" class="key white"><h1>D</h1></div>
    <div data-note="F" class="key white"><h1>F</h1></div>
    <div data-note="Gb" class="key black"><h2>R</h2></div>
    <div data-note="G" class="key white"><h1>G</h1></div>
    <div data-note="Ab" class="key black"><h2>T</h2></div>
    <div data-note="A" class="key white"><h1>H</h1></div>
    <div data-note="Bb" class="key black"><h2>Y</h2></div>
    <div data-note="B" class="key white"><h1>J</h1></div>
    <div data-note="C1" class="key white"><h1>K</h1></div>
    <div data-note="Db1" class="key black"><h2>U</h2></div>
    <div data-note="D1" class="key white"><h1>L</h1></div>
    <div data-note="Eb1" class="key black"><h2>O</h2></div>
    <div data-note="E1" class="key white"><h1>;</h1></div>
  </div>

  </div>

  <audio id="C" src="notes/C.mp3"></audio>
  <audio id="Db" src="notes/Db.mp3"></audio>
  <audio id="D" src="notes/D.mp3"></audio>
  <audio id="Eb" src="notes/Eb.mp3"></audio>
  <audio id="E" src="notes/E.mp3"></audio>
  <audio id="F" src="notes/F.mp3"></audio>
  <audio id="Gb" src="notes/Gb.mp3"></audio>
  <audio id="G" src="notes/G.mp3"></audio>
  <audio id="Ab" src="notes/Ab.mp3"></audio>
  <audio id="A" src="notes/A.mp3"></audio>
  <audio id="Bb" src="notes/Bb.mp3"></audio>
  <audio id="B" src="notes/B.mp3"></audio>
  <audio id="C1" src="notes/C.mp3"></audio>
  <audio id="Db1" src="notes/Db.mp3"></audio>
  <audio id="D1" src="notes/D.mp3"></audio>
  <audio id="Eb1" src="notes/Eb.mp3"></audio>
  <audio id="E1" src="notes/E.mp3"></audio>
</body>
</html>